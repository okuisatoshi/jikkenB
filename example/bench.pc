int main()
{
    int n; int c; int i; int j; int k;
    //scan n;
    n = 3;
    c = 0;
    // n回反復
    i = n;
    while (i > 0) {
        // n回反復
        j = n;
        while (j > 0) {
            // n回反復
            k = n;
            while (k > 0) {
                n = n % 32768 + 1;
                c = c + 1;
                k = k - 1;
            }
