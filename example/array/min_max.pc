// 配列の例

int main()
{
    int a[8];
    a[0] = 3; a[1] = 1; a[2] = 4; a[3] = 1;
    a[4] = 5; a[5] = 9; a[6] = 2; a[7] = 6;

    int i; int min; int max;
    min = a[0];
    max = a[0];
    i = 1;
    while (i < 8) {
        if (a[i] < min) min = a[i];
        if (a[i] > max) max = a[i];
        i = i + 1;
    }
    print min;
    print max;
}

